{
    "buildCommand": "next build",
    "cleanUrls": true,
    "function" :{
        "user/register":{
            "memory":3008,
            "maxDuration":60
        },
        "user/login":{
            "memory":3008,
            "maxDuration":60
        }
    },
    "version":2,
    "builds":[
        {
            "src": "index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"

        }
    ]
}